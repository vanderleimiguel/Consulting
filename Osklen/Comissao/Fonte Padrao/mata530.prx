	EndIf  
	
	If MV_PAR13 == 1 .And. FWModeAccess("SE3",3)=="E" .And. FWModeAccess("SE2",3)=="E"  // Seleciona filiais
		Processa( { |lEnd| fa530Fil(MV_PAR14,MV_PAR15) })
	Else
		Processa( { |lEnd| fa530Processa() }) 
	EndIf

EndIf

Return(.T.)

